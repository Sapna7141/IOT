const int led1 = 9;
const int led2 = 2;


void setup() {
pinMode(led1,OUTPUT);
pinMode(led2,OUTPUT);
Serial.begin(9600);
}

void loop() {
  
digitalWrite(led1,HIGH);
digitalWrite(led2,HIGH);
Serial.println("both leds are ON");
delay(1000);
digitalWrite(led1,LOW);
digitalWrite(led2,LOW);
Serial.println("both leds are OFF");
delay(1000);
}
